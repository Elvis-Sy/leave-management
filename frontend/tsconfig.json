    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "components/sidebar/sidebar-item.jsx", "components/sidebar/collapse-items.jsx", "components/sidebar/companies-dropdown.jsx", "components/sidebar/sidebar-menu.jsx", "components/sidebar/sidebar.styles.ts", "components/sidebar/sidebar.jsx", "components/navbar/burguer-button.jsx", "components/navbar/darkmodeswitch.jsx", "components/navbar/navbar.jsx", "components/navbar/notifications-dropdown.jsx", "components/navbar/user-dropdown.jsx", "components/home/demandeCard.jsx", "components/home/card-balance2.jsx", "components/home/card-balance3.jsx", "components/home/content.jsx", "components/charts/tauxChart.jsx", "components/auth/authLayout.jsx", "components/auth/login.jsx", "components/table/render-cell.jsx", "components/table/table.jsx", "components/accounts/index.jsx", "app/(app)/page.tsx", "app/(app)/(admin)/managers/page.tsx", "components/auth/register.jsx"  ],
